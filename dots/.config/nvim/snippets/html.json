{
  "a:blank": {
    "prefix": "a:blank",
    "abbreviation" : "a[href='http://${0}' target='_blank' rel='noopener noreferrer']",
    "body": "<a href=\"http://${1}\" target=\"_blank\" rel=\"noopener noreferrer\">${2}</a>",
    "description": "Emmet abbreviation",
    "scope": "text.html"
  },
  "linkcss": {
    "prefix": "link:css",
    "body": "<link rel=\"stylesheet\" href=\"${1:style}.css\">",
    "description": "Emmet abbreviation",
    "scope": "text.html"
  }
}
